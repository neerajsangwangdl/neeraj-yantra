{
  "name": "yantra-world",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "dev": "ng serve",
    "build": "ng build --configuration production --base-href=/ --aot --output-hashing=all",
    "test": "web-push generate-vapid-keys --json",
    "http": "http-server -p 8081 -c-1 dist/",
    "e2e": "ng e2e",
    "start-pwa": "ng build --configuration production && http-server -p 8081 -c-1 dist/yantra-world",
    "build-prod": "node --max_old_space_size=5048 ./node_modules/@angular/cli/bin/ng build --configuration production --build-optimizer --vendor-chunk=true --output-hashing=all",
    "heroku-postbuild": "ng build --configuration production --aot --output-Hashing=all"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.3",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/localize": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@angular/service-worker": "^14.1.3",
    "bootstrap": "^5.2.0",
    "browser-image-compression": "^2.0.0",
    "font-awesome": "^4.7.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "local-iso-dt": "^3.1.0",
    "ng-multiselect-dropdown": "^1.0.0-beta.19",
    "ngx-infinite-scroll": "^14.0.0",
    "ngx-toastr": "^15.0.0",
    "node.js": "0.0.1-security",
    "nprogress": "^0.2.0",
    "rxjs": "~7.5.0",
    "rxjs-compat": "^6.6.7",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.3",
    "@angular/cli": "~14.1.3",
    "@angular/compiler-cli": "^14.1.0",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^18.7.11",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
